{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "TerraformStateS3ListBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketVersioning"
      ],
      "Resource": "arn:aws:s3:::terraform-state-436204347828-us-east-1"
    },
    {
      "Sid": "TerraformStateS3ReadWrite",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:GetObjectVersion"
      ],
      "Resource": "arn:aws:s3:::terraform-state-436204347828-us-east-1/*"
    }
  ]
}
